generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ImageFile {
  id             Int       @id @default(autoincrement())
  filePath       String    @unique
  title          String?
  description    String?
  photoTakenDate DateTime?
  createdDate    DateTime  @default(now())
}

model User {
  id        Int    @id @default(autoincrement())
  email     String @unique
  firstName String
  lastName  String
  password  String
}
